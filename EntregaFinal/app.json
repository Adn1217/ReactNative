{
  "expo": {
    "name": "toDoApp",
    "slug": "toDoApp",
    "version": "2.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adn1217.toDoApp",
      "versionCode": 1.0,
      "permissions": ["ACCESS_COARSE_LOCATION", "CAMERA"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAw0Fl9pIBYZEJtAK_g8jaVuSCnuMMt06o"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1eff195c-21dc-436f-8315-c429cd6f4934"
      }
    }
  }
}
